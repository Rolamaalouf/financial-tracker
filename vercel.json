{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://fin-sync-1.onrender.com/api/:path*"
      },
      {
        "source": "/(?!api/.*)",
        "destination": "/index.html"
      }
    ]
  }
  